{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": "off",
      "no-plusplus": "off",
      "prettier/prettier": "error"
    }
}
